{
  "name": "bank-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.5.0",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "http-proxy-middleware": "^2.0.6",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^9.1.3"
  },
  "scripts": {
    "postinstall": "react-dotenv && npm run copy-libs",
    "copy-libs": "cpx node_modules/bootstrap/dist/**/* src/lib -u -C",
    "start": "react-dotenv && react-scripts start",
    "build": "react-dotenv && react-scripts build",
    "test": "react-dotenv &&react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cpx": "^1.5.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "react-dotenv": {
    "whitelist": [
      "API_URL"
    ]
  }
}
